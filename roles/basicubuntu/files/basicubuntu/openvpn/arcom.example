client
port 1199
proto tcp
dev tun1
remote nomeclientinf.dnsalias.net
resolv-retry infinite
nobind
persist-key
persist-tun
ca /etc/openvpn/nomeclient/ca.crt
cert /etc/openvpn/nomeclient/cetrificate.crt
key /etc/openvpn/nomeclient/keyfile.key
ns-cert-type server
comp-lzo
verb 3
ping 15
ping-restart 300
persist-key
#reneg-sec 18000
